{
  "default": {
    "paths": ["tests/features/*.feature"],
    "require": [
      "tests/step-definitions/*.ts",
      "tests/hooks/hooks.ts"
    ],
    "format": [
      "@cucumber/pretty-formatter",
      "json:reports/cucumber-report.json"
    ],
    "requireModule": ["ts-node/register"],
    "timeout": 300000
  }
}